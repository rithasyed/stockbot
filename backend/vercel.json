{
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/main.py"
        }
    ],
    "rewrites": [
          {
            "source": "/(.*)", 
            "destination": "/"}
        ]
}
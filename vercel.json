{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python@3.1.0",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "pip": [
          "--no-cache-dir"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ]
}